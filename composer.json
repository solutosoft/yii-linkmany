{
  "name": "solutosoft/yii-linkmany",
  "description": "Load, validate and save automatically related Active Record models",
  "keywords": [
    "yii2",
    "active-record",
    "behavior",
    "relation",
    "fill",
    "load"
  ],
  "homepage": "https://github.com/solutosoft/yii-linkmany",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Leandro Guindani Gehlen",
      "email": "leandrogehlen@gmail.com",
      "homepage": "https://github.com/leandrogehlen"
    }
  ],
  "support": {
    "issues": "https://github.com/solutosoft/yii-linkmany/issues?state=open",
    "source": "https://github.com/solutosoft/yii-linkmany"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "solutosoft\\linkmany\\": "src",
      "solutosoft\\linkmany\\tests\\": "tests"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
